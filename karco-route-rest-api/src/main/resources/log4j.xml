<Configuration status="debug" name="karco-rest-api" packages="">
	<Appenders>
		<Console name="stdout" target="SYSTEM_OUT">
			<PatternLayout pattern="%d{yyyy-MM-dd HH:mm:ss} %p %m%n"/>
		</Console>
		<File name="fout" fileName="karcorestapi.log" append="true">
			<PatternLayout>
				<Pattern>%d{yyyy-MM-dd HH:mm:ss} %-5p %m%nw</Pattern>
			</PatternLayout>
		</File>
	</Appenders>
	<Root level="INFO">
		<AppenderRef ref="stdout"/>
		<AppenderRef ref="fout"/>
	</Root>
</Configuration>